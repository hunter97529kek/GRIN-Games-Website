	function mouse_over( obj, name ) {
		document.getElementById( obj ).className = name+"_hover";
	}
	function mouse_out( obj, name ) {
		document.getElementById( obj ).className = name;
	}
	
	function mouse_over_obj( obj, name ) {
		obj.className = name+"_hover";
	}
	function mouse_out_obj( obj, name ) {
		obj.className = name;
