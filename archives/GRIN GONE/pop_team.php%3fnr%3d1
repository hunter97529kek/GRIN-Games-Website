	function launch( url, theName, w, h ) {
		var x=100;
		var y=100;
		param='width='+w+',height='+h+',location=no,resizable=no,menubar=no,scrollbars=no,status=no,toolbar=no,titlebar=no,left='+x+',top='+y;
